<div class="container-fluid">
  <div class="row">
    <div class="col-std">
      
    </div>
  </div>
  <div class="row">
    <div class="col-std">
      <h1>Projects</h1>
      <a href="<%= new_admin_project_path %>" class="btn btn-default">Create a new project</a>
      <% @projects.each do |project| %>
      <div class="project">
        <h2><%= project.title %></h2>
        <p><%= project.description %></p>
        <a href="<%= edit_admin_project_path(project) %>" class="btn btn-default">Edit</a>
      </div>
      <% end %>
    </div>
  </div>
</div>